<!DOCTYPE html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Bán chậu cảnh | Trang chủ</title>

    <!-- Favicon  -->
    <link rel="icon" href="~/Content/img/core-img/favicon.ico">
    <!-- Core Style CSS -->
    <link rel="stylesheet" href="~/Content/css/core-style.css">
    <link rel="stylesheet" href="~/Content/style.css">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="main-content-wrapper d-flex clearfix">
        @Html.Partial("_Header")

        <div class="container body-content">
            @RenderBody()
            <hr />
        </div>
    </div>
    @Html.Partial("_Footer")
    <script src="~/Scripts/js/jquery/jquery-2.2.4.min.js"></script>
    <!-- Popper js -->
    <script src="~/Scripts/js/popper.min.js"></script>
    <!-- Bootstrap js -->
    <script src="~/Scripts/js/bootstrap.min.js"></script>
    <!-- Plugins js -->
    <script src="~/Scripts/js/plugins.js"></script>
    <!-- Active js -->
    <script src="~/Scripts/js/active.js"></script>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)


</body>

